pipeline
{
	agent any
	stages
	{
		
		stage ('scm checkout')
		{
			steps
			{
				git branch: 'master', url: 'https://github.com/sachin1753/hello-pipeline'
			}

		}
	

		stage ('list s3 bucket')
		{
			steps
			{
				withCredentials([usernameColonPassword(credentialsId: 'aws-key', variable: '')]) {
				    AWS("--region=us-east-1 s3 ls")
				}
			}

		}
		

	}
}
